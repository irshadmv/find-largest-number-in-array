#include <stdio.h>

int main() {
    int A[20],i,j,n,sum,k;
    printf("enter the number of elements to be stored");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        printf("enter the %d num",i+1);
        scanf("%d",&A[i]);
    }
    sum=0;
    for(k=0;k<n;k++)
    {
        if(A[k]>sum)
        {
            sum=A[k];
        }
    }
    printf("%d is large",sum);
    
    
    return 0;
}
